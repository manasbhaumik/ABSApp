@{
    ViewBag.Title = "APS Change Password";
}
<div class="row">
    <div class="col-md-2"><img class="img-responsive" src="~/Assets/images/cnb_logo.png" /></div><div class="col-md-10">   <h2>@ViewBag.Title.</h2></div>
</div>
<div class="row">
<br/><br/>
</div>
    <div class="row">
        <div class="col-md-12">
            <section id="loginForm">


                <div class="form-group col-md-6">
                    @Html.Label("Successfully reset password", new { @class = "col-md-12 control-label" })
                    @Html.Label("Please check your email or sms for your new password", new { @class = "col-md-12 control-label" })
                </div>
                <div class="row"><br /></div>

                <div class="row"><br /><br /></div>
                <div class="form-group">
                    <div class="col-md-2"></div>
                    <div class="col-md-12">
                        <input type="submit" value="Ok" class="btn btn-default" onclick="location.href='@Url.Action("APSLogin", "APS")'" />
                    </div>
                </div>           

            </section>
        </div>

    </div>
